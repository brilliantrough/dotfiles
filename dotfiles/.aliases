# User specific aliases and functions
# To ignore alias, run cmd prepened with \
# or disable an alias altogether with unalias
# To get alias def, just use alias name
# alias name='' or name="" both ok
clsourquotes() {
	clear
	source ~/.randomquotes;
}
alias c='clsourquotes'
alias ll='ls -ahlF'
alias la='ls -A'
alias l='ls -CF'
alias sl='sl -e'
alias mv='mv -i'
alias mkdir='mkdir -p'
alias df='df -h'
# alias sed='sed -E'
alias v='vim'
alias ty='typora'
alias tm='tmux'
alias tms='tmux new -s'
alias tma='tmux attach -t' # = tmux a -t
alias tmls='tmux ls'
alias gs='git status'
alias gp='git push'
alias gpl='git pull'
alias gad='git add .'
alias gcm='git commit -m'
